exec dbo.FS_GetDayLog

exec cdw..Getlog_SP 1000 


--Bank Dw 2.0:

select * 
from [MAL].[log].[v_FileMonitorExecution]
--where IsExecutedToday = 1
where FileMonitorStartDTS > '2020-10-18' AND FileMonitorStartDTS < '2020-10-19'
--order by JobCode, FileMonitorStartDTS desc, JobStartDTS desc
order by FileMonitorStartDTS desc, JobStartDTS desc

select * 
from [MAL].[log].[v_FileMonitorExecution]
where IsExecutedToday = 1
--order by JobCode, FileMonitorStartDTS desc, JobStartDTS desc
order by FileMonitorStartDTS desc, JobStartDTS desc


select * 
from [MAL].[log].[v_SQLAgentJob]
where IsLatestExecution = 1
order by SQLAgentJobName                      



select JobPackageStatusName,* 
from mal.[log].[v_JobAndPackageExecution] 
--where jobname='BOSSE till DW'
WHERE IsLatestJobExecution = 1
--WHERE StartDTS > '2020-10-23'
order by JobStartDts desc
