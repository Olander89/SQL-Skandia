exec cdw..Getlog_SP 1000
exec mart..FS_GetMonthLog

SELECT 
 fil_data_dat	= fil_data_dat,
 Antal	= COUNT(1)
FROM sestdbs121.Staging.dbo.stgLojalitetskund
WHERE fil_data_dat > DATEADD(d,-10,GETDATE())
GROUP BY fil_data_dat
ORDER BY fil_data_dat DESC



