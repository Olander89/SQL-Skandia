/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [Kundreferens]
      ,[Ärendenummer]
      ,[Produkt]
      ,[Typ_av_eftergift]
      ,[Startdatum_eftergift]
      ,[Slutdatum_eftergift]
      ,[Belopp]
      ,[Beloppstyp]
      ,[Pågående_Avslutad]
      ,[LoadDate]
      ,[ExtractDate]
      ,[FileName]
  FROM [STAGE].[dbo].[Raw_Inkasso_Eftergifter]

  SELECT DISTINCT LoadDate FROM [STAGE].[dbo].[Raw_Inkasso_Eftergifter]

  SELECT COUNT(*) FROM [STAGE].[dbo].[Raw_Inkasso_Eftergifter]

/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [SourceSystem]
      ,[Event]
      ,[EventDate]
      ,[MasterPackageExecutionId]
      ,[InfoColumn1]
  FROM [CDW].[dbo].[FSLoadStatus_Monthly]
  WHERE SourceSystem = 'Lindorff'
  ORDER BY EventDate DESC
